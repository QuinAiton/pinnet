<body>
  <!-- Sidebar  -->
  <div class="sidebar">
      <a href="/" class="sidebar-header">
        <div class="logo-icon"></div>
        <h1>pinnet</h1>
    </a>

    <% if (userId) { %>

    <div class="menu">
      <a href="/">
        <button>Explore</button>
      </a>
      <a href="/users/<%= userId %>">
        <button>My Pins</button>
      </a>
    </div>

    <div class="logins">
      <a href="/logout">
        <button>Logout</button>
      </a>
    </div>

    <% } else { %>

      <div class="menu">
        <a href="/">
          <button>Explore</button>
        </a>
       </div>

      <div class="logins">
        <a href="/login">
          <button>Login</button>
        </a>
        <a href="/register">
          <button>Register</button>
        </a>
      </div>

    <% } %>


  </div>
</body>
